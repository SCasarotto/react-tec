{
	"name": "react-tec",
	"version": "0.4.3",
	"license": "MIT",
	"author": "Stuart Casarotto",
	"main": "dist/index.js",
	"module": "dist/react-tec.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test --env=jsdom",
		"lint": "tsdx lint",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"build-and-deploy": "yarn run build-storybook -o ./docs/ --no-dll && yarn run s3-deploy && yarn run set-cache-control && echo 'ðŸš€ Deployed!!!'",
		"s3-deploy": "s3cmd sync ./docs/* s3://www.react-tec.com --acl-public --delete-removed --guess-mime-type --no-mime-magic --no-preserve --cf-invalidate-default-index",
		"set-cache-control": "s3cmd modify --add-header 'Cache-Control: no-cache, must-revalidate' s3://www.react-tec.com/index.html"
	},
	"peerDependencies": {
		"react": ">=16",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"styled-components": "^5.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint && yarn test"
		}
	},
	"prettier": {
		"bracketSpacing": true,
		"singleQuote": true,
		"arrowParens": "always",
		"trailingComma": "all",
		"semi": false,
		"useTabs": true,
		"tabWidth": 4,
		"jsxBracketSameLine": false,
		"printWidth": 100,
		"jsxSingleQuote": true
	},
	"devDependencies": {
		"@babel/core": "7.11.6",
		"@storybook/addon-actions": "^6.0.28",
		"@storybook/addon-essentials": "^6.0.28",
		"@storybook/addon-links": "^6.0.28",
		"@storybook/react": "^6.0.28",
		"@testing-library/react": "11.1.0",
		"@types/jest": "^26.0.15",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"@types/react-helmet": "^6.1.0",
		"@types/react-icons": "^3.0.0",
		"@types/react-router-dom": "^5.1.6",
		"@types/styled-components": "^5.1.4",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.1.0",
		"hook": "^0.0.5",
		"husky": "^4.2.5",
		"jest-styled-components": "^7.0.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-is": "^17.0.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"styled-components": "^5.2.1",
		"tsdx": "0.14.1",
		"tslib": "2.0.3",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"@popperjs/core": "^2.5.4",
		"@testing-library/jest-dom": "^5.11.5",
		"@types/color": "^3.0.1",
		"@types/react-avatar-editor": "^10.3.5",
		"@types/react-datepicker": "^3.1.1",
		"@types/react-phone-number-input": "^3.0.5",
		"@types/react-select": "^3.0.23",
		"color": "^3.1.3",
		"react-avatar-editor": "11.0.12",
		"react-datepicker": "^3.3.0",
		"react-helmet": "^6.1.0",
		"react-icons": "^3.10.0",
		"react-phone-number-input": "^3.1.3",
