	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build-and-deploy": "yarn run build && yarn run s3-deploy && yarn run set-cache-control && echo 'ðŸš€ Deployed!!!'",
		"s3-deploy": "s3cmd sync build/* s3://www.react-tec.com --acl-public --delete-removed --guess-mime-type --no-mime-magic --no-preserve --cf-invalidate-default-index",
		"set-cache-control": "s3cmd modify --add-header 'Cache-Control: no-cache, must-revalidate' s3://www.react-tec.com/index.html"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"singleQuote": true,
		"arrowParens": "always",
		"trailingComma": "all",
		"semi": false,
		"useTabs": true,
		"tabWidth": 4
	}
